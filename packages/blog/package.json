{
    "name": "frsource.github.io-blog",
    "private": true,
    "version": "1.0.0",
    "description": "Source for FRSOURCE blog",
    "repository": "https://github.com/FRSource/frsource.github.io.git",
    "author": "Jakub Freisler",
    "license": "UNLICENSED FOR ANY USE",
    "scripts": {
        "start": "concurrently \"pnpm sync-posts:watch\" \"pnpm docs:start\"",
        "build": "pnpm sync-posts && pnpm docs:build && pnpm copy-production-dist",
        "sync-posts": "node -r @swc-node/register ./.vitepress/scripts/syncPosts.ts",
        "sync-posts:watch": "node -r @swc-node/register ./.vitepress/scripts/syncPosts.ts -w",
        "docs:start": "vitepress dev .",
        "docs:build": "vitepress build .",
        "docs:preview": "vitepress preview .",
        "format": "prettier --write .",
        "copy-production-dist": "cpy . ../../../../public/blog  --cwd .vitepress/dist"
    },
    "keywords": [],
    "devDependencies": {
        "@swc-node/register": "^1.5.6",
        "@swc/core": "1.3.25",
        "@types/command-line-args": "5.2.0",
        "@types/cross-spawn": "6.0.2",
        "@types/fs-extra": "11.0.1",
        "@types/lodash": "4.14.191",
        "@types/remove-markdown": "0.3.1",
        "chokidar": "3.5.3",
        "command-line-args": "5.2.1",
        "concurrently": "7.6.0",
        "cpy-cli": "4.2.0",
        "cross-spawn": "7.0.3",
        "deep-object-diff": "1.1.9",
        "fs-extra": "11.1.0",
        "gray-matter": "4.0.3",
        "lodash": "4.17.21",
        "vitepress": "1.0.0-alpha.45",
        "vue": "3.2.47"
    },
    "dependencies": {
        "simple-icons": "8.1.0"
    }
}
